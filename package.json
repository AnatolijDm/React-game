{
  "name": "react-game",
  "version": "1.0.0",
  "description": "Full stack game",
  "main": "app.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node app.js",
    "server": "nodemon app.js",
    "client": "npm run start --prefix client",
    "clientInstall": "npm install --prefix client",
    "clientBuild": "npm run build  --prefix client",
    "dev": "cross-env NODE_ENV=development concurrently \"npm run server --max-http-header-size=16384\" \"npm run client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AnatolijDm/React-game.git"
  },
  "keywords": [
    "react",
    "node",
    "express",
    "mongoDB"
  ],
  "author": "Anatolij Dmitruk <anatolij.dmitruk@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AnatolijDm/React-game/issues"
  },
  "homepage": "https://github.com/AnatolijDm/React-game#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "config": "^3.3.3",
    "eslint": "^7.20.0",
    "express": "^4.17.1",
    "express-validator": "^6.9.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.11.17"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.7"
  }
}
